{
  "name": "@app-x/frontend",
  "version": "0.0.1",
  "description": "frontend",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch \"../../packages/webpack-config/\" --exec \"webpack serve --config ../../packages/webpack-config/dev.js\"",
    "build:dev": "webpack -c ../../packages/webpack-config/dev.js",
    "build:prod": "webpack -c ../../packages/webpack-config/prod.js",
    "build:test": "webpack -c ../../packages/webpack-config/test.js",
    "dev:simple": "webpack serve --config ../../packages/webpack-config/dev.js",
    "lint:js": "eslint ./src",
    "lint:vue": "eslint ./src --ext .js,.vue",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint --max-warnings 2",
    "*.vue": "eslint --max-warnings 2"
  },
  "eslintConfig": {
    "extends": [
      "../../packages/eslint-config/vue.json"
    ]
  },
  "browserslist": [
    "> 0.01%",
    "last 15 versions",
    "ie 8",
    "ie 9",
    "ie 10",
    "Firefox < 20",
    "Safari < 7",
    "Android 4"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@mdi/font": "^7.4.47",
    "eventemitter3": "^5.0.1",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.10.3",
    "vuex": "^4.1.0"
  }
}
